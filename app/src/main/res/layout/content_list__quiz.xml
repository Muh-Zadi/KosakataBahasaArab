<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_list__quiz"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/dsn_bg_quiz"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.zadi.kosakatabahasaarab.List_Quiz"
    tools:showIn="@layout/activity_list__quiz">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/itempelay" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingRight="@dimen/activity_horizontal_margin">

        <ListView
            android:id="@+id/mobilelist"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </RelativeLayout>
</RelativeLayout>
